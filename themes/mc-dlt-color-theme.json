      "settings": {
        "fontStyle": "",
        "foreground": "#ffffce"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "support.variable.automatic.powershell"],
      "settings": {
        "foreground": "#ffffce"
      }
    },
    {
      "name": "Invalid – Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic underline",
