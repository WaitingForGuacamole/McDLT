  "tokenColors": [
    {
      "settings": {
        "foreground": "#bdae9d"
      }
    },
    {
      "name": "Other",
      "scope": ["meta.scriptblock.powershell"],
      "settings": {
        "foreground": "#bdae9d"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "${secondaryForeground}b0"
      }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": {
        "foreground": "${constantValue}"
      }
    },
    {
      "name": "Entity",
      "scope": "entity",
      "settings": {
        "fontStyle": "",
        "foreground": "#72c3fc"
      }
    },
    {
      "name": "Function",
      "scope": [
        "function",
        "support.function.powershell",
        "entity.name.function.powershell"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#43fafa"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "fontStyle": "",
        "foreground": "${keywordName}"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "${keywordName}"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted.double.powershell"],
      "settings": {
        "fontStyle": "",
        "foreground": "${constantValue}"
      }
    },
    {
      "name": "Number",
      "scope": ["Number", "constant.numeric.integer.powershell"],
      "settings": {
        "fontStyle": "",
        "foreground": "${constantValue}"
      }
    },
    {
      "name": "Support",
      "scope": "support",
      "settings": {
        "fontStyle": "",
        "foreground": "${primaryForeground}"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "support.variable.automatic.powershell"],
      "settings": {
        "foreground": "${primaryForeground}"
      }
    },
    {
      "name": "Invalid – Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "${errorForeground}"
      }
    },
    {
      "name": "Invalid – Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#a0a0a0"
      }
    },
    {
      "name": "attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "${secondaryForeground}"
      }
    },
    {
      "name": "tag",
      "scope": [
        "entity.name.tag",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end"
      ],
      "settings": {
        "foreground": "#37A8ED"
      }
    },
    {
      "name": "css property-value",
      "scope": [
        "meta.property-group support.constant.property-value.css",
        "meta.property-value support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "${constantValue}"
      }
    }
  ]
}
